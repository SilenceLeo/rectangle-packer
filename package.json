{
    "name": "rectangle-packer",
    "version": "1.0.2",
    "author": "SilenceLeo <ly.liyang521@gmail.com>",
    "description": "Rectangle Packer",
    "keywords": [
        "min rectangle pack",
        "rectangle packer",
        "rectang pack",
        "packer 2D",
        "pack 2D"
    ],
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/types/index.d.ts"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/SilenceLeo/rectangle-packer.git",
        "directory": "SilenceLeo/rectangle-packer"
    },
    "bugs": {
        "url": "https://github.com/SilenceLeo/rectangle-packer/issues"
    },
    "scripts": {
        "build": "node build.js",
        "dev": "tsc --watch --project tsconfig.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint src/**/*.ts --fix",
        "fix": "npm run lint && npm run format",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^8.35.1",
        "@typescript-eslint/parser": "^8.35.1",
        "eslint": "^9.30.1",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.0",
        "typescript": "^5.0.0"
    }
}
